<h1>Sign Up</h1>
<%= form_for @user do |f| %>
  <%=f.label :fname, "First Name" %>
  <%=f.text_field :fname %>
  <br>
  <%=f.label :lname, "Last Name" %>
  <%=f.text_field :lname %>
  <br>
  <%=f.label :email, "Email" %>
  <%=f.text_field :email %>
  <br>
  <%=f.label :password, "Password" %>
  <%=f.password_field :password %>
  <br> 
  <%=f.submit :create %> 
  <!-- OR you can use a string bc submit doesnt take a form -->
<% end %>